# Environment configuration for Time Nest (example values)
FLASK_ENV=development
FLASK_DEBUG=1

SECRET_KEY=dev-secret-key-change-me
JWT_SECRET_KEY=your-jwt-secret-here
JWT_EXPIRES_HOURS=12

MONGO_URI=mongodb://localhost:27017/?directConnection=true
MONGO_DB_NAME=timenest

# Google OAuth 2.0 (fill these with real values from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
# If you change this URL, also update the OAuth client redirect URI in Google Cloud
GOOGLE_REDIRECT_URI=http://127.0.0.1:5000/auth/google/callback
